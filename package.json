{
  "name": "next-gen-front-end-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@appbaseio/reactivesearch": "^3.4.3",
    "@emotion/core": "^10.0.28",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@reglendo/canvas2image": "^1.0.5-2",
    "cookie": "^0.4.0",
    "cross-fetch": "^3.0.3",
    "elasticsearch": "^16.6.0",
    "honeybadger-js": "^2.1.3",
    "jest-util": "25.1.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.2",
    "openseadragon": "^2.4.2",
    "react": "^16.13.0",
    "react-app-polyfill": "^1.0.6",
    "react-collapsible": "^2.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.0",
    "react-notify-toast": "^0.5.0",
    "react-protected-mailto": "^1.0.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.0",
    "react-scripts": "^3.4.0",
    "react-select": "^3.0.8",
    "react-share": "^4.0.1",
    "react-sizes": "^2.0.0",
    "react-slidedown": "^2.4.1",
    "react-spinners": "^0.8.1",
    "react-tabs": "^3.1.0",
    "react-test-renderer": "^16.13.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.2.0",
    "source-map-explorer": "^2.3.1",
    "swiper": "^5.3.6",
    "whatwg-fetch": "^3.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "cypress/**/*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start --no-cache",
    "start": "HTTPS=${HTTPS:-true} HOST=${HOST:-devbox.library.northwestern.edu} PORT=${PORT:-3333} SSL_CRT_FILE=$SSL_CERT SSL_KEY_FILE=$SSL_KEY npm-run-all -p watch-css start-js",
    "start:use-real-data": "REACT_APP_DONUT_URL=https://donut.library.northwestern.edu/ REACT_APP_ELASTICSEARCH_PROXY_BASE=https://dcapi.stack.rdc.library.northwestern.edu REACT_APP_LIVE_IIIF=true npm-run-all -p start",
    "start:use-staging-data": "REACT_APP_DONUT_URL=https://donut.stack.rdc-staging.library.northwestern.edu/ REACT_APP_ELASTICSEARCH_PROXY_BASE=https://dcapi.stack.rdc-staging.library.northwestern.edu REACT_APP_LIVE_IIIF=true npm-run-all -p start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --check src/**/*.js* src/**/*.css ./*.js ./*.json",
    "prettier-fix": "prettier --write src/**/*.js* src/**/*.css ./*.js ./*.json",
    "ci": "react-scripts test --watchAll=false --coverage",
    "cypress:mock-auth": "CYPRESS_ELASTICSEARCH_PROXY_BASE=https://dcapi.stack.rdc.library.northwestern.edu cypress open"
  },
  "devDependencies": {
    "@nulib/prettier-config": "^1.2.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^25.1.4",
    "coveralls": "^3.0.9",
    "cypress": "4.3.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.17.1",
    "prop-types": "^15.6.1",
    "react-router-prop-types": "^1.0.4",
    "start-server-and-test": "^1.10.11"
  },
  "prettier": "@nulib/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
