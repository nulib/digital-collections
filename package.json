{
  "name": "next-gen-front-end-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@appbaseio/reactivesearch": "^2.8.1",
    "@emotion/core": "^10.0.10",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "cookie": "^0.3.1",
    "cross-fetch": "^3.0.1",
    "elasticsearch": "^15.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "honeybadger-js": "^0.5.5",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "openseadragon": "^2.4.0",
    "prop-types": "^15.6.1",
    "react": "^16.8.2",
    "react-collapsible": "^2.3.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-notify-toast": "^0.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.5",
    "react-share": "^2.2.0",
    "react-sizes": "^1.0.4",
    "react-slidedown": "^2.0.4",
    "react-spinners": "^0.5.1",
    "react-tabs": "^3.0.0",
    "react-test-renderer": "^16.8.2",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "source-map-explorer": "^1.6.0",
    "swiper": "^4.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "HOST=devbox.library.northwestern.edu PORT=3333 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.13.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "jest-util": "v24.2.0-alpha.0"
  }
}
