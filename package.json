{
  "name": "next-gen-front-end-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@appbaseio/reactivesearch": "^3.1.1",
    "@emotion/core": "^10.0.22",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.0-11",
    "@reglendo/canvas2image": "^1.0.5-2",
    "cookie": "^0.4.0",
    "cross-fetch": "^3.0.3",
    "elasticsearch": "^16.5.0",
    "honeybadger-js": "^2.0.0",
    "jest-util": "24.8.0",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.2",
    "openseadragon": "^2.4.0",
    "react": "^16.8.2",
    "react-app-polyfill": "^1.0.1",
    "react-collapsible": "^2.3.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "react-notify-toast": "^0.5.0",
    "react-protected-mailto": "^1.0.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.0",
    "react-scripts": "^3.0.1",
    "react-select": "^3.0.8",
    "react-share": "^3.0.0",
    "react-sizes": "^2.0.0",
    "react-slidedown": "^2.4.1",
    "react-spinners": "^0.6.1",
    "react-tabs": "^3.0.0",
    "react-test-renderer": "^16.8.2",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "source-map-explorer": "^2.0.0",
    "swiper": "^5.2.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "HOST=devbox.library.northwestern.edu PORT=3333 react-scripts start --no-cache",
    "start": "npm-run-all -p watch-css start-js",
    "start:use-real-data": "export REACT_APP_DONUT_URL=https://donut.library.northwestern.edu/ && export REACT_APP_ELASTICSEARCH_PROXY_BASE=https://5et90kbva4.execute-api.us-east-1.amazonaws.com/latest/ && export REACT_APP_LIVE_IIIF=true && npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --check src/**/*.js* src/**/*.css ./*.js ./*.json",
    "prettier-fix": "prettier --write src/**/*.js* src/**/*.css ./*.js ./*.json",
    "ci": "react-scripts test --watchAll=false --coverage"
  },
  "devDependencies": {
    "@nulib/prettier-config": "^1.2.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.20",
    "coveralls": "^3.0.5",
    "cypress": "^4.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.17.1",
    "prop-types": "^15.6.1",
    "react-router-prop-types": "^1.0.4"
  },
  "prettier": "@nulib/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
